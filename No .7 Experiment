import pandas as pd

data = {"CustomerID":[1,1,2,3],
        "OrderDate":["2025-01-01","2025-01-05","2025-01-03","2025-01-06"],
        "Product":["A","B","A","C"],
        "Quantity":[2,1,5,3]}

df = pd.DataFrame(data)

print(df.groupby("CustomerID").size())
print(df.groupby("Product")["Quantity"].mean())
print("Earliest:", df["OrderDate"].min())
print("Latest:", df["OrderDate"].max())
