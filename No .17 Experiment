import pandas as pd
import string
from collections import Counter
import matplotlib.pyplot as plt

df = pd.read_csv("data.csv")

text = " ".join(df["feedback"]).lower()
for p in string.punctuation:
    text = text.replace(p,"")

words = text.split()
freq = Counter(words)

top = freq.most_common(5)
print(top)

w,v = zip(*top)
plt.bar(w,v)
plt.show()
